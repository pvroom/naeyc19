<ion-header>
    <ion-toolbar>
        <ion-buttons slot="start">
            <ion-back-button [defaultHref]="defaultHref"></ion-back-button>
          </ion-buttons>
       <ion-title>Presenter Details</ion-title>
       </ion-toolbar>
   </ion-header>

  <ion-content>
  
    <div style="background:#0F3B5F">
      <div>
          <div>
              <img class="avatar" [src]="visualImageURL" onerror="this.src='assets/img/personIcon.png'"
                  src="assets/img/personIcon.png" alt="Image error">
                  </div>
  
              </div>
              <div>
  
                  <div style="margin-top:15px; text-align: center!important; color:#fff">
                      <h2>{{visualDisplayName}}</h2>
                      <h4>{{visualAffiliation}}</h4>
                      <button *ngIf="btnBookmarkManagement" (click)="BookmarkManagement()" color="tertiary">
                        <div>
                          <ion-icon name="bookmark"></ion-icon>
                          <label>{{visBookmarkAddRemoveButton}}</label>
                        </div>
                      </button>
      
                  </div>
          </div>
  
  </div>
  
    <div>
      <div>
        Biography
      </div>
      <div [innerHTML]="spkrDetails" style="margin-top:5px">
        {{visualBiography}}
      </div>
    </div>
  
    <div *ngIf="WebsiteShow">
      <div>
        <p style="color:#fff">
          Website
        </p>
      </div>
  
      <div class="list" (click)="navToWebsite(visWebsite)">
          {{visWebsite}}
      </div>
    </div>
  
    <div>
      <div>
          Sessions
      </div>
    <div>
    
   
          <div lines="none" id="speakersessions-list3" (click)="EventDetails(session.EventID)" *ngFor="let session of SessionListing" id="speakersessions-list-item19">
          <div
              [innerHTML]="session.DisplaySessionnHeader">
            </div>
           <ion-icon slot="end" color="tertiary" name="arrow-dropright"></ion-icon>
           <ion-icon slot="start" color="tertiary" name="list"></ion-icon>
          
          </div>
        </div>

    
    </div>
  
  </ion-content>
  