<ion-header>
	<ion-toolbar>
		<ion-buttons slot="start">
			<ion-back-button></ion-back-button>
		</ion-buttons>
		<ion-title>Networking</ion-title>
	</ion-toolbar>
</ion-header>
  
  <ion-content class="networking" style="background:#425195">
  
    <ion-grid>
    
      <ion-row>
        <ion-col size="6">
          <ion-button no-lines detail-none expand="full" class="buttonShape" style="background-color:#F15D22" (click)="NavigateTo('Attendees')" id="program-list-item1">
            <ion-icon name="contacts" class="buttonIconsL"></ion-icon>
            <h2>Attendees</h2>
          </ion-button>
        </ion-col>
  
        <ion-col size="6">
          <ion-button no-lines detail-none expand="full" class="buttonShape" style="background:#FDB515"  (click)="NavigateTo('Conversations')" id="program-list-item3">
            <ion-icon name="chatbubbles" class="buttonIconsL"></ion-icon>
            <h2>Conversations</h2>
            <p *ngIf=NewMessagesIndicator style="color:#fff;">{{NewMessagesCounter}} New Messages</p>
          </ion-button>
        </ion-col>
      </ion-row>
      
      <ion-row> 
        <ion-col size="6">
          <ion-button no-lines detail-none expand="full" class="buttonShape" style="background:#0F3B5F;" (click)="NavigateTo('MyProfile')" id="program-list-item5">
            <ion-icon name="contact" class="buttonIconsL"></ion-icon>
            <h2>My Profile</h2>
          </ion-button>
        </ion-col>
        
        <ion-col size="6">
          <ion-button no-lines detail-none expand="full" class="buttonShape" style="background:#F15D22!important" (click)="NavigateTo('Notifications')" id="program-list-item2">
            <ion-icon name="alert" class="buttonIconsL"></ion-icon>
            <h2>Notifications</h2>
          </ion-button>
        </ion-col>
      </ion-row>
      
      <ion-row>
        <ion-col size="6">
          <ion-button no-lines detail-none expand="full" class="buttonShape" style="background:#FDB515" (click)="NavigateTo('ActivityFeed')" id="program-list-item5">
            <ion-icon name="chatboxes" class="buttonIconsL"></ion-icon>
            <h2>Activities Feed</h2>
          </ion-button>
        </ion-col>
        
        <ion-col size="6">
          <ion-button no-lines detail-none expand="full" class="buttonShape" style="background:#0F3B5F" (click)="NavigateTo('SocialMedia')" id="program-list-item2">
            <ion-icon name="text" class="buttonIconsL"></ion-icon>
            <h2>Social Media</h2>
          </ion-button>
        </ion-col>
      </ion-row>
        
    </ion-grid>
  
  </ion-content>
  
  
  
  