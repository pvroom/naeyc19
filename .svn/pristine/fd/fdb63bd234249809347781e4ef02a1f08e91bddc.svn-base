<ion-split-pane>
    <ion-menu contentId="content">
      <ion-header>
        <ion-toolbar>
          <ion-title>Menu</ion-title>
        </ion-toolbar>
      </ion-header>
  <ion-content>

        <ion-list style="background: url(./assets/img/guitar.jpg); color:#F15D22; margin:0; padding:0">
          <ion-menu-toggle autoHide="false" *ngFor="let p of pages">
            <ion-item [routerLink]="p.url" routerDirection="root" tappable style="background: rgba(0, 0, 0, 0.5); 
        color:#fff; margin:0; padding:0">
                <ion-icon style="color:#F15D22" name="{{p.icon}}" slot="start"></ion-icon>
                {{p.title}}
            </ion-item>
</ion-menu-toggle>
</ion-list>





<ion-list (click)="expandItem(item)" *ngFor="let item of items">
  <ion-item style="background: rgba(0, 0, 0, 0.5); color:#fff" routerLink="/menu/social">Networking</ion-item>
     <app-expandable expandHeight="300px" [expanded]="item.expanded">
        
             
            <ion-item style="background:transparent!important; color:#444" routerLink="/menu/myagendafull">Full Agenda</ion-item>
            <ion-item style="background:transparent!important; color:#444" routerLink="/menu/attendees">Attendees</ion-item>
            <ion-item style="background:transparent!important; color:#444" routerLink="/menu/login">Login</ion-item>
            <ion-item style="background:transparent!important; color:#444" routerLink="/menu/more">More</ion-item>
            <ion-item style="background:transparent!important; color:#444" routerLink="/menu/networking">Networking</ion-item>
            <ion-item style="background:transparent!important; color:#444" routerLink="/menu/notes">Notes</ion-item>
            <ion-item style="background:transparent!important; color:#444" routerLink="/menu/speakers">Speakers</ion-item>
    
     
     </app-expandable>
   
       </ion-list>
   

          </ion-content>
          </ion-menu>

          <ion-router-outlet id="content" main></ion-router-outlet>

        </ion-split-pane>


