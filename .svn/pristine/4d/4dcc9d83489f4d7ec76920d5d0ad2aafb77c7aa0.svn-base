      <ion-card>
        <ion-card-header class="ion-text-wrap" style="background:#510e88">	
          <h2 style="color:#fff">4.1) Please contribute your additional comments or suggestions about this course below.</h2>
        </ion-card-header>
      
        <ion-card-content>
          <ion-textarea (input)='CEEvaluationQ41 = $event.target.value' 
            name="CEEvaluationQ41"
            [value]="CEEvaluationQ41" 
            id="CEEvaluationQ41"
            placeholder="Enter text" rows="4"></ion-textarea>
        </ion-card-content>
      </ion-card>
