    <div>
      <ion-button style="background:#710080; width:50%; margin-right:25%; margin-left:25%" (click)="SubmitEvaluation()">
        SUBMIT
      </ion-button>
    </div>
    <br/><br/><br/>
  
  </ion-content>
  
  
  
  