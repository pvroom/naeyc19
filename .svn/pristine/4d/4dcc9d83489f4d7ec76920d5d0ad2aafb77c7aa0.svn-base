      <ion-card>
  
        <ion-card-header class="ion-text-wrap" style="background:#510e88">	
          <h2 style="color:#fff">3.3) The balance of heavy emphasis on hands-on and minimal emphasis on lecture was good.</h2>
        </ion-card-header>
  
        <ion-card-content>
          <ion-list radio-group [(ngModel)]="CEEvaluationQ33" name="CEEvaluationQ33">
            <ion-item>
              <ion-label>5 - Strongly Agree</ion-label>
              <ion-radio value="5"></ion-radio>
            </ion-item>
            <ion-item>
              <ion-label>4</ion-label>
