<ion-header>
    <ion-toolbar>
        <ion-buttons slot="start">
            <ion-menu-button></ion-menu-button>
            </ion-buttons>
		<ion-title>NAEYC 2019 Nashville</ion-title>
		<ion-avatar slot="end" style="margin:5px;" (click)="NavigateToLoginPage()">
			<text-avatar *ngIf=AttendeeLoggedIn text="{{AttendeeInitials}}"></text-avatar>
			<ion-icon style="height:100%; width:100%;font-size:32px;" *ngIf=AttendeeLoggedOut name="{{LogInOutIcon}}"></ion-icon>
		</ion-avatar>
	</ion-toolbar>

		</ion-header>


	
		<div style="background:rgb(221, 220, 220); margin-top:-15; margin-bottom:0; padding-top:0; padding-bottom:0">
			<h4 style="text-align:center; color:#F15D22">Sign our petition to the Governor! <ion-checkbox></ion-checkbox></h4>
			<ion-button color="tertiary" fill="solid" size="small" style="margin-left:auto; 
			margin-right:auto; margin-bottom:10px; display:block; width:50%" routerLink="/petition">Read more here...</ion-button>
				</div>



