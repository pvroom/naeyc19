<ion-header>
    <ion-toolbar>
        <ion-menu-button slot="start"></ion-menu-button>
       <ion-title>Full Agenda</ion-title>
       </ion-toolbar>
   </ion-header>
  
  <ion-content>
  
  

    <!--Rotating Banner Ad
    <div style="background:rgb(255, 255, 255)">
      <ion-slides class="ad" autoplay="5000" loop="true" speed="3000">
        <ion-slide>
          <img src="assets/img/shope.jpg">
        </ion-slide>
        <ion-slide >
          <img src="assets/img/cafe.png">
        </ion-slide>
      </ion-slides>
    </div>
  
  -->
  
    <!--Segment Menu Bar-->
    <div>
      <ion-segment [(ngModel)]="agenda">
        <ion-segment-button value="myagenda" style="background:#0F3B5F; color:#fff">
          My Agenda
        </ion-segment-button>
        <ion-segment-button value="fullagenda" style="background:#F15D22; color:#fff">
          Full Agenda
        </ion-segment-button>
      </ion-segment>
    </div>
  
    <!--My Agenda Contents-->
    <div [ngSwitch]="agenda">
      <ion-list *ngSwitchCase="'myagenda'" id="agendafull-list3">
  
          <!-- Tuesday agenda items -->
          <ion-card>
            <ion-card-header style="background:#0F3B5F; color:#fff">
              Tuesday, Nov 19
            </ion-card-header>
            <ion-card-content>
              <ion-item (click)="EventDetails(agendaTue.visEventID)" *ngFor="let agendaTue of agendaTueItems" id="fullagenda-list-item19" >
                <div>
                  <h2 style="font-weight:600">{{agendaTue.EventName}}</h2>
                  <p>{{agendaTue.visEventTimeframe}}</p>
                  <p>{{agendaTue.EventLocation}}</p>
                  <p [ngClass]="agendaTue.SessionStatusStyle">{{agendaTue.SessionStatus}}</p>
                </div>
                <ion-icon color="tertiary" slot="start" name="{{agendaTue.eventTypeIcon}}"></ion-icon>
              </ion-item>
            </ion-card-content>
          </ion-card>
  
          <!-- Wednesday agenda items -->
          <ion-card>
            <ion-card-header style="background:#0F3B5F; color:#fff">
              Wednesday, Nov 20
            </ion-card-header>
            <ion-card-content>
              <ion-item class="ion-text-wrap" (click)="EventDetails(agendaWed.visEventID)" *ngFor="let agendaWed of agendaWedItems" id="fullagenda-list-item19" >
                <ion-icon color="tertiary" slot="start" name="{{agendaWed.eventTypeIcon}}"></ion-icon>
              <div>
                <h2 style="font-weight:600">{{agendaWed.EventName}}</h2>
                <p>{{agendaWed.visEventTimeframe}}</p>
                <p>{{agendaWed.EventLocation}}</p>
                <p [ngClass]="agendaWed.SessionStatusStyle">{{agendaWed.SessionStatus}}</p>
              </div>
              </ion-item>
            </ion-card-content>
          </ion-card>
  
  
          <!-- Thursday agenda items -->
          <ion-card>
            <ion-card-header style="background:#0F3B5F; color:#fff">
             Thursday, Nov 21
            </ion-card-header>
