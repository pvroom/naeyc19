              <ion-item class="ion-text-wrap" (click)="EventDetails(agendaThu.visEventID)" *ngFor="let agendaThu of agendaThuItems" id="fullagenda-list-item20">
                <ion-icon color="tertiary" slot="start" name="{{agendaThu.eventTypeIcon}}"></ion-icon>
