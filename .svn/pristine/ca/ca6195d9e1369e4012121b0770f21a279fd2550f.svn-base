<ion-header>
    <ion-toolbar>
       <ion-buttons slot="start">
        <ion-back-button></ion-back-button>
       </ion-buttons>
       <ion-title>Sponsors</ion-title>
       </ion-toolbar>
   </ion-header>



    
    <ion-content>
       
      <!-- Search bar -->
      <ion-grid>
        <ion-row>
          <ion-col col-9>	
    
            <ion-item class="item-input; shadow">
              <ion-icon name="search" item-left></ion-icon>
              <ion-input name="srchBarEntry" id="srchBarEntry" 
                type="text" placeholder="Search" [(ngModel)]="EntryTerms"></ion-input>
            </ion-item>
          </ion-col>
          <ion-col col-3>
            <ion-button ion-button block color=secondary style="margin:0" (tap)="GetSearchResults()">Submit</ion-button>
          </ion-col>
        </ion-row>
      </ion-grid>
    



      <ion-grid>
          <ion-row>
            <ion-col>
              <div>
                1 of 3
              </div>
            </ion-col>
            <ion-col>
              <div>
                2 of 3
              </div>
            </ion-col>
            <ion-col>
              <div>
                3 of 3
              </div>
            </ion-col>
          </ion-row>
        </ion-grid>







    
      <!-- Sponsor level headers-->
    
      <ion-card *ngFor="let sponsor of SponsorListing">
    
      <!--
