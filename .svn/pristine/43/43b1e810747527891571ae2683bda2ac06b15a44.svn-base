<ion-header>
    <ion-toolbar>
        <ion-buttons slot="start">
          <ion-back-button [defaultHref]="defaultHref"></ion-back-button>
            </ion-buttons>
       <ion-title>Attendees</ion-title>
       </ion-toolbar>

   </ion-header>
    
    <ion-content class="attendees-page">
    
      <!-- Attendee Listing -->
      <ion-grid>
        <ion-row>
          <ion-col size="11">	
            <ion-list>
              <ion-item (click)="AttendeeDetails(attendee.AttendeeID)" *ngFor="let attendee of AttendeeListing" id="Attendee-list19">
                  <div style="float: left; padding-right: 15px;">
                  <ion-avatar slot="start">
                    <img *ngIf=attendee.ShowHideAttendeeIcon [src]="attendee.AttendeeAvatar" 
                    src="assets/img/personIcon.png" onerror="this.src='assets/img/personIcon.png'">
                  </ion-avatar>
                </div>
                <!--<ion-icon slot="end" color="tertiary" name="{{attendee.navigationArrow}}"></ion-icon> -->
                  <div [innerHTML]="attendee.DisplayBlock">
                  </div>
              </ion-item>
            </ion-list>
         
          </ion-col>
    
            <ion-col size="1" style="position:fixed; top: 15%; right: 0; z-index:10" >
    
              <ion-button style="text-align:center; margin:0; height:20px; width:35px;padding-right:5px" [ngClass]="btnA" (click)="LoadAlpha('A')">
                <ion-label>A</ion-label>
              </ion-button>
    
