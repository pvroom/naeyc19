<ion-header>
    <ion-toolbar>
       <ion-buttons slot="start">
        <ion-back-button></ion-back-button>
       </ion-buttons>
       <ion-title>Attendees</ion-title>
       </ion-toolbar>
   </ion-header>


    
    <ion-content class="attendees-page">

<!--NEW SEARCHBAR-->
        <ion-searchbar (ionInput)="getItems($event)"></ion-searchbar>
        <ion-list>
          <ion-item *ngFor="let item of items">
            {{ item }}
          </ion-item>
        </ion-list>
    


      <ion-grid style="padding:0; margin:0">
        <ion-row>
          <ion-col col="10">	
            <ion-item class="item-input">
              <ion-icon color="secondary" name="search" item-left></ion-icon>
              <ion-input name="srchBarEntry" id="srchBarEntry" 
              type="text" placeholder="Search" [(ngModel)]="EntryTerms"></ion-input>
            </ion-item>
          </ion-col>
          <ion-col col="2">
            <ion-button style="padding:0; margin:0; background:#43ada5" (tap)="GetSearchResults()">Submit</ion-button>
          </ion-col>
        </ion-row>
      </ion-grid>
    
      <!-- Attendee Listing -->
      <ion-grid>
        <ion-row>
          <ion-col col="11">	
            <ion-list>
              <ion-item tappable (click)="AttendeeDetails(attendee.AttendeeID)" *ngFor="let attendee of AttendeeListing" id="Attendee-list19">
                  <div style="float: left; padding-right: 10px;">
                  <ion-avatar slot="start">
                    <img *ngIf=attendee.ShowHideAttendeeIcon [src]="attendee.AttendeeAvatar" src="assets/img/personIcon.png" onerror="this.src='assets/img/personIcon.png'">
                  </ion-avatar>
                </div>
                <ion-icon slot="end" name="{{attendee.navigationArrow}}"></ion-icon>
                  <div [innerHTML]="attendee.DisplayBlock">
                  </div>
              </ion-item>
            </ion-list>
          </ion-col>
    
    
    
    
    
        
          
            <ion-col col="1" style="position:sticky; position: -webkit-sticky; /* Safari */; top:0; right:0">
    
