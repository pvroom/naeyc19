                <img src="assets/img/austinap.jpeg">
              </ion-thumbnail>
              <ion-label>Travel</ion-label>
            </ion-item>

  

            <ion-item tappable class="item-avatar item-icon-right" href="#" onclick="window.open('https://www.naeyc.org/events/annual/2019', '_system', 'location=yes'); return false;">
