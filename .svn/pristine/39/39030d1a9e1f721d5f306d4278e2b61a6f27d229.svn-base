<ion-header>
    <ion-toolbar>
        <ion-buttons slot="start">
            <ion-menu-button></ion-menu-button>
            </ion-buttons>
       <ion-title>Notes</ion-title>
       </ion-toolbar>
   </ion-header>
   

<ion-content>



  
    <ion-list id="Notes-list3">
  
      <!-- Saturday note items -->
      <ion-card>
  
        <ion-card-header>
          Tuesday, November 19
        </ion-card-header>
  
        <ion-card-content>
          <ion-item (click)="NoteDetails(noteTue.EventID)" *ngFor="let noteTue of notesTue" id="notes-list-item19">
            <ion-icon slot="start" color="tertiary" name="create"></ion-icon>
            <ion-icon slot="end" color="tertiary" name="arrow-dropright"></ion-icon>
            <div>
            <h2>
              {{noteTue.CourseName}}
            </h2>
            <p>
              {{noteTue.visEventTimeframe}}
            </p>
            <p>
              {{noteTue.NoteBeginning}}
            </p>
          </div>
          </ion-item>
        </ion-card-content>
  
      </ion-card>
  
      <!-- Sunday note items -->
      <ion-card>
  
        <ion-card-header>
          Wednesday, November 20
        </ion-card-header>
  
